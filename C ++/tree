#include<bits/stdc++.h>

using namespace std;

struct node{
int data;
struct node* left;
struct node* right;
};

struct node* newnode(int data){

struct node* node=(struct node*) malloc(sizeof(struct node));
node->data=data;
node->left=NULL;
node->right=NULL;
return (node);
}


int root(struct node* root){

if (root=NULL){
    return 0;
}
else{
        return count(root->left)+count(root->right)+1;
    
}
bool checkrec(struct node* root,int n){
if (root==NULL){
    return false;
}
if (count(root)=n-count(root)){
    return true;
}
return (checkrec(root->left,n) || checkrec(root->right,n));   
}

